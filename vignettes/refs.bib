@article{Breslow_1981,
     jstor_articletype = {research-article},
     title = {Odds Ratio Estimators When the Data are Sparse},
     author = {Breslow, Norman},
     journal = {Biometrika},
     jstor_issuetitle = {},
     volume = {68},
     number = {1},
     jstor_formatteddate = {Apr., 1981},
     pages = {pp. 73-84},
     url = {http://www.jstor.org/stable/2335807},
     ISSN = {00063444},
     abstract = {The properties of four commonly used estimators of the odds ratio are studied under a large-sample scheme in which the number of 2 × 2 tables increases but the possible marginal configurations remain fixed. Neither the unconditional maximum likelihood nor the empirical logit estimators converge to the true odds ratio; their asymptotic bias is computed for certain special cases of interest. The conditional maximum likelihood and Mantel-Haenszel estimators, which are consistent, have asymptotic variances whose ratio tends to one under the null hypothesis. Even for moderately large odds ratios the simple Mantel-Haenszel estimator maintains remarkably good efficiency.},
     language = {English},
     year = {1981},
     publisher = {Biometrika Trust},
     copyright = {Copyright © 1981 Biometrika Trust},
    }

    @article{Breslow_1982,
     jstor_articletype = {research-article},
     title = {The Variance of the Mantel-Haenszel Estimator},
     author = {Breslow, N. E. and Liang, K. Y.},
     journal = {Biometrics},
     jstor_issuetitle = {},
     volume = {38},
     number = {4},
     jstor_formatteddate = {Dec., 1982},
     pages = {pp. 943-952},
     url = {http://www.jstor.org/stable/2529874},
     ISSN = {0006341X},
     abstract = {Four variances for the logarithm of the Mantel-Haenszel estimator of the common odds ratio in a series of 2 X 2 tables are studied by means of Monte Carlo experiments. One variance is appropriate when the sample sizes in each table are large, while another is derived with the assumption that the number of tables increases. Neither is adequate when the underlying assumptions are violated. However, a weighted average of the two, as well as a jackknife variance, perform well and lead to reasonably accurate tests and confidence intervals over a range of different sampling schemes. Inferences based on the empirical logit estimator are shown to be distorted in comparison, due to bias when the true odds ratio departs from unity.},
     language = {English},
     year = {1982},
     publisher = {International Biometric Society},
     copyright = {Copyright © 1982 International Biometric Society},
    }

    @article{lowess_1988,
     jstor_articletype = {research-article},
     title = {Locally Weighted Regression: An Approach to Regression Analysis by Local Fitting},
     author = {Cleveland, William S. and Devlin, Susan J.},
     journal = {Journal of the American Statistical Association},
     jstor_issuetitle = {},
     volume = {83},
     number = {403},
     jstor_formatteddate = {Sep., 1988},
     pages = {pp. 596-610},
     url = {http://www.jstor.org/stable/2289282},
     ISSN = {01621459},
     abstract = {Locally weighted regression, or loess, is a way of estimating a regression surface through a multivariate smoothing procedure, fitting a function of the independent variables locally and in a moving fashion analogous to how a moving average is computed for a time series. With local fitting we can estimate a much wider class of regression surfaces than with the usual classes of parametric functions, such as polynomials. The goal of this article is to show, through applications, how loess can be used for three purposes: data exploration, diagnostic checking of parametric models, and providing a nonparametric regression surface. Along the way, the following methodology is introduced: (a) a multivariate smoothing procedure that is an extension of univariate locally weighted regression; (b) statistical procedures that are analogous to those used in the least-squares fitting of parametric functions; (c) several graphical methods that are useful tools for understanding loess estimates and checking the assumptions on which the estimation procedure is based; and (d) the M plot, an adaptation of Mallow's C<sub>p</sub> procedure, which provides a graphical portrayal of the trade-off between variance and bias, and which can be used to choose the amount of smoothing.},
     language = {English},
     year = {1988},
     publisher = {American Statistical Association},
     copyright = {Copyright © 1988 American Statistical Association},
    }

